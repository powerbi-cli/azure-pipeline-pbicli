{
    "manifestVersion": 1,
    "id": "pbicli",
    "version": "0.9.2",
    "name": "Power BI CLI",
    "publisher": "pbicli",
    "description": "This extension adds the Power BI CLI (https://powerbi-cli.github.io/) to the pipeline",
    "tags": ["azure", "deployment", "power bi", "pbicli"],
    "scopes": ["vso.release"],
    "branding": {
        "color": "rgb(87, 32, 31)",
        "theme": "dark"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": ["Azure Pipelines"],
    "icons": {
        "default": "./logo.png"
    },
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/powerbi-cli/azure-pipeline-pbicli"
        },
        "support": {
            "uri": "https://github.com/powerbi-cli/azure-pipeline-pbicli/issues"
        },
        "privacypolicy": {
            "uri": "https://azurebi-docs.jppp.org/vsts-extensions/privacy-terms.html"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/powerbi-cli/azure-pipeline-pbicli"
    },
    "contributions": [
        {
            "id": "pbicli",
            "type": "ms.vss-distributed-task.task",
            "targets": ["ms.vss-distributed-task.tasks"],
            "properties": {
                "name": "pbicli"
            }
        }
    ],
    "files": [
        {
            "path": "pbicli/dist"
        },
        {
            "path": "pbicli/node_modules"
        },
        {
            "path": "pbicli/task.json"
        },
        {
            "path": "pbicli/icon.png"
        }
    ]
}
